<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.newtouch.dms.repository.RoleProfileDao" >
  <resultMap id="BaseResultMap" type="RoleProfileKey" >
    <id column="role_id" property="roleId" jdbcType="INTEGER" />
    <id column="profile_id" property="profileId" jdbcType="INTEGER" />
  </resultMap>
  <delete id="deleteByPrimaryKey" parameterType="RoleProfileKey" >
    delete from pdc_role_profile
    where role_id = #{roleId,jdbcType=INTEGER}
      and profile_id = #{profileId,jdbcType=INTEGER}
  </delete>
  <insert id="insert" parameterType="RoleProfileKey" >
    insert into pdc_role_profile (role_id, profile_id)
    values (#{roleId,jdbcType=INTEGER}, #{profileId,jdbcType=INTEGER})
  </insert>
  <insert id="insertSelective" parameterType="RoleProfileKey" >
    insert into pdc_role_profile
    <trim prefix="(" suffix=")" suffixOverrides="," >
      <if test="roleId != null" >
        role_id,
      </if>
      <if test="profileId != null" >
        profile_id,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides="," >
      <if test="roleId != null" >
        #{roleId,jdbcType=INTEGER},
      </if>
      <if test="profileId != null" >
        #{profileId,jdbcType=INTEGER},
      </if>
    </trim>
  </insert>
</mapper>